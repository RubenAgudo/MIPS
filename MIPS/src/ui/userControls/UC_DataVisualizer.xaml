<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:oxy="http://oxyplot.codeplex.com"
             xmlns:ViewModels="clr-namespace:MIPS.src.ViewModels" 
			 x:Class="MIPS.src.ui.userControls.UC_DataVisualizer"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" >
	<Grid >

		<oxy:PlotView x:Name="oxyplot" HorizontalContentAlignment="Stretch" 
					VerticalContentAlignment="Stretch" IsManipulationEnabled="True" >
		</oxy:PlotView>
	</Grid>

</UserControl>
