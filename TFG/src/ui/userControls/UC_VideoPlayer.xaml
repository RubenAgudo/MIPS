<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="TFG.src.ui.userControls.UC_VideoPlayer"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    mc:Ignorable="d" 
    d:DesignHeight="300" d:DesignWidth="300">

    <Grid x:Name="grid" x:FieldModifier="private">
        
        <MediaElement  
			x:Name="myMediaElement" 
			Margin="11,0,9,106" 
			RenderTransformOrigin="0.5,0.5" 
			LoadedBehavior="Manual" IsMuted="True" UnloadedBehavior="Manual" ScrubbingEnabled="True" MediaEnded="myMediaElement_MediaEnded"/>
    </Grid>
</UserControl>
